{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org/"
    },
    {
      "type": "git",
      "url": "https://github.com/pixelant/pxa_form_enhancement"
    },
    {
      "type": "git",
      "url": "https://github.com/t3kit/t3kit_docker"
    },
    {
      "type": "git",
      "url": "https://github.com/t3kit/t3kit_extension_tools"
    },
    {
      "type": "git",
      "url": "https://github.com/t3kit/theme_t3kit"
    },
    {
      "type": "git",
      "url": "https://github.com/t3kit/theme_t3kit_bluemountain"
    },
    {
      "type": "git",
      "url": "https://bitbucket.org/pixelant/pxa_newsletter_subscription"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5, <7.1",
    "typo3/cms": "^7.6",
    "typo3-ter/theme-t3kit": "1.9.0",
    "typo3-ter/theme-t3kit-bluemountain": "1.0.0",
    "georgringer/news": "4.3.0",
    "clickstorm/go_maps_ext": "2.0.0",
    "typo3-ter/realurl": "2.0.14",
